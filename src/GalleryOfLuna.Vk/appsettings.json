{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss}] [{SourceContext:l}] [{Level}] {Message}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": [
      "FromLogContext"
    ]
  },

  "Database": {
    "ConnectionString": "Data Source=GalleryOfLuna.Publishing.db;"
  },

  "Derpibooru": {
    "ConnectionString": "Server=localhost;Port=5433;Database=derpibooru;User Id=postgres;Password=Pass123$;"
  },

  "Targets": [
    {
      "Name": "Target name",
      "Description": "",
      "Query": "princess luna",
      "Schedule": "L B C"
    }
  ]
}
