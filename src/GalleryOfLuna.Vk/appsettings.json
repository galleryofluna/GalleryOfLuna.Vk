{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Warning"
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss}] [{SourceContext:l}] [{Level}] {Message}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": [
      "FromLogContext"
    ]
  },

  "Database": {
    "ConnectionString": "Data Source=GalleryOfLuna.Publishing.db;"
  },

  "Derpibooru": {
    "ConnectionString": "Insert here connection string to PostgreSQL with dump of Derpibooru database"
  } 
}
